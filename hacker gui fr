			PluginsFrame:TweenPosition(UDim2.new(0, 0, 0, 0), "InOut", "Quart", 0.5, true, nil)
			wait(0.5)
			disablebuttons()
		else
			notify('Incompatible Exploit','Your exploit is unable to use plugins')
		end
	end)

