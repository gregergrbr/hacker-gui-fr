		TextBox_Roundify_12px_2.Size = UDim2.new(1, 0, 1, 0)
		TextBox_Roundify_12px_2.Image = "rbxassetid://2260429633"
		TextBox_Roundify_12px_2.ImageColor3 = Color3.new(0.0823529, 0.0823529, 0.0823529)
